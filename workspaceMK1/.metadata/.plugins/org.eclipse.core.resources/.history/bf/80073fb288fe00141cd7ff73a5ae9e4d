package javaModel;

public class DobreSuciastky extends Suciastky {

	@Override
	public int setCena(int Pocet) // Vypocet ceny pre dobre suciastky
	{
		Pocet = Suciastky.getPocet();
		Cena = (Pocet * 500);
		return Cena;
	}
	public int setCas(int Pocet)
	{
		Pocet = Suciastky.getPocet();
		
		CasPrac.cas=(Pocet*4);
		return CasPrac.cas;
	}
}
